<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cyuxuan.dao.test.TestDao">
    <select id="selectNameById" resultType="String">
        SELECT name FROM bee_mybatis WHERE id = #{id,jdbcType=INTEGER}
    </select>

    <select id="selectSexById" resultType="String">
        SELECT sex FROM bee_mybatis WHERE id = #{id,jdbcType=INTEGER}
    </select>
</mapper>